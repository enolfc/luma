{
    "dirs_config": {
        "cluster_manager": {
            "input_dir": "rel/cluster_manager",
            "target_dir": "rel/test_cluster"
        },
        "op_worker": {
            "input_dir": "rel/op_worker",
            "target_dir": "rel/test_cluster"
        },
        "globalregistry": {
            "input_dir": "rel/globalregistry",
            "target_dir": "rel/test_cluster"
        }
    },
    "os_configs": {
        "cfg1": {
            "storages": [
                "/mnt/st1"
            ],
            "users": [
                "user1"
            ],
            "groups": {
                "group1": [
                    "user1"
                ]
            }
        }
    },
    "provider_domains": {
        "p1": {
            "db_driver": "couchbase",
            "os_config": "cfg1",
            "cluster_manager": {
                "cm1": {
                    "vm.args": {
                        "setcookie": "cookie1"
                    },
                    "sys.config": {
                        "cm_nodes": [
                            "cm1"
                        ],
                        "worker_num": 1
                    }
                }
            },
            "op_worker": {
                "worker1": {
                    "vm.args": {
                        "setcookie": "cookie1"
                    },
                    "sys.config": {
                        "cm_nodes": [
                            "cm1"
                        ],
                        "db_nodes": [
                            "dbnode1"
                        ],
                        "verify_gr_cert": false
                    }
                }
            }
        }
    },
    "globalregistry_domains": {
        "gr": {
            "globalregistry": {
                "node1": {
                    "vm.args": {
                        "setcookie": "cookie3"
                    },
                    "sys.config": {
                        "db_nodes": [
                            "db"
                        ],
                        "http_domain": {
                            "string": "127.0.0.1"
                        },
                        "dev_mode": true
                    }
                }
            }
        }
    }
}